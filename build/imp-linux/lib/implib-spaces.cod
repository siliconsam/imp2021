{----------------------------------------------------------------------------}
    %external %routine spaces(%integer count)
 0000                      L 1000  EQU $
 0000 C8 00 00 01                     ENTER 0000, 1
        %while count > 0 %cycle
 0004                      L 1001  EQU $
 0004 8B 45 08                        MOV EAX,[EBP+ 8]
 0007 3D 00 00 00 00                  CMP EAX, 0
 000C 7E 00                           JLE L 1002
            printsymbol(' ')
 000E 6A 20                           PUSH  32
 0010 E8 2A 00                        CALL 'PRINTSYMBOL' (EXTERN  42)
 0013 83 C4 04                        ADD ESP, 4
            count = count - 1
 0016 FF 4D 08                        DEC WORD [EBP+ 8]
        %repeat
 0019 EB 00                           JMP L 1001
 001B                      L 1002  EQU $
    %end
 001B C9                              LEAVE
 001C C3                              RET

{----------------------------------------------------------------------------}
%endoffile
      _TEXT  ENDS
      CONST  SEGMENT WORD PUBLIC 'CONST'
      CONST  ENDS
      _TEXT  SEGMENT WORD PUBLIC 'CODE'
            ENDS
      DATA  SEGMENT WORD PUBLIC 'DATA'
      DATA    ENDS
              ENDS
      _SWTAB  SEGMENT WORD PUBLIC '_SWTAB'
      _SWTAB   ENDS
