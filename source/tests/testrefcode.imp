%begin
    %constant %integer useimp = 0
    %constant %integer useintel = 1

    %external %integer bilbo
    %external %integer %function %spec baggins ( %integer gandalf, mode )

    %integer t

    %for t=1,1,5 %cycle
        bilbo = 2*t
        printstring("baggins(".itos(t,0).")=".itos(baggins(t,useimp),0))
        newline
    %repeat

    newline
    printstring("Loop unrolling gives:");newline

    bilbo = 2
    printstring("baggins(".itos(1,0).")=".itos(baggins(1,useintel),0))
    newline

    bilbo = 4
    printstring("baggins(".itos(2,0).")=".itos(baggins(2,useintel),0))
    newline

    bilbo = 6
    printstring("baggins(".itos(3,0).")=".itos(baggins(3,useintel),0))
    newline

    bilbo = 8
    printstring("baggins(".itos(4,0).")=".itos(baggins(4,useintel),0))
    newline

    bilbo = 10
    printstring("baggins(".itos(5,0).")=".itos(baggins(5,useintel),0))
    newline

%endofprogram
